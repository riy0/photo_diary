<ul class="users">
  <h1>All Users</h1>
  <% @users.each do |user| %>
    <li>
      <% if user.image_url %>
        <%= image_tag user.image_url, size: "36x36" %>
      <% else %>
        <%= image_tag 'default-user.jpeg', size:"36x36" %>
      <% end %>
      <%= link_to user.name, user %>
    </li>
   <% end %>
</ul> 
